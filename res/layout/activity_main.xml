<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity" >

    <Button
        android:id="@+id/app_open_app"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/app_open_app" />

    <Button
        android:id="@+id/wap_open_app"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/wap_open_app" />

    <Button
        android:id="@+id/msg_open_app"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/msg_open_app" />
    
    
    <Button
        android:id="@+id/weibsite_filter"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/weibsite_filter" />

</LinearLayout>
